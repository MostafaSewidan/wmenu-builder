{
    "name": "tocaan/laravel-menu",
    "description": "drag and drop menu generator like wordpress for laravel 5",
    "keywords": ["Laravel", "Menu", "Wordpress", "Builder"],
    "authors": [
    	{
            "name": "Rendy tocaan",
            "email": "rendytocaan@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Tocaan World",
            "email": "wmenu@tocaan.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "5.* || 6.* || 7.* || 8.*"
    },
    "autoload": {
        "psr-4": {
          "Tocaan\\Menu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tocaan\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Tocaan\\Menu\\Facades\\Menu"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
